<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<AnchorPane prefHeight="416.0" prefWidth="805.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="il.cshaifasweng.OCSFMediatorExample.client.ShowExamController">
    <children>
        <HBox layoutX="-1.0" prefHeight="331.0" prefWidth="806.0">
            <children>
                <TableView fx:id="QuestionsTable" prefHeight="331.0" prefWidth="806.0">
                </TableView>
            </children>
        </HBox>
        <VBox layoutX="14.0" layoutY="331.0" spacing="10" />
      <Button fx:id="manualExamButton" layoutX="84.0" layoutY="340.0" mnemonicParsing="false" onAction="#ManualExam" prefHeight="61.0" prefWidth="264.0" text="Execute as Manual" />
      <Button fx:id="autoExamButton" layoutX="438.0" layoutY="340.0" mnemonicParsing="false" onAction="#AutoExam" prefHeight="61.0" prefWidth="264.0" text="Execute as Automatic" />
    </children>
</AnchorPane>
